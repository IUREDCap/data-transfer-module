{
	"name" : "iu-redcap/data-transfer",
	"license" : "BSD 3-Clause",
	"description" : "Data Transfer Module",
	"keywords" : [
		"REDCap"
	],
    "config": {
        "vendor-dir": "dev"
    },
    "require-dev": {
        "phpunit/phpunit": "10.5.13",
        "squizlabs/php_codesniffer": "3.9.0",
        "vimeo/psalm": "5.23.1",
        "erusev/parsedown": "1.7.4",
        "phpstan/phpstan": "2.1.0"
    },
	"autoload" : {
		"psr-4" : {
			"IU\\DataTransfer\\" : ["./", "./classes/"]
		}
	},
    "autoload-dev": {
        "psr-4" : {
            "IU\\DataTransfer\\": [
                "tests/unit"
            ],
            "ExternalModules\\": [
                "tests/external"
            ]
        }
    }
}
